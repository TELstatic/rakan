{
  "name": "telstatic/rakan",
  "description": "Laravel 文件系统扩展,适配 阿里OSS,七牛云",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "TELstatic",
      "email": "telstatic@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "hashids/hashids": "3.0.0",
    "aliyuncs/oss-sdk-php": "^2.3",
    "league/flysystem": "^1.0",
    "qcloud/cos-sdk-v5": ">=2.0",
    "qiniu/php-sdk": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "TELstatic\\Rakan\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TELstatic\\Rakan\\RakanServiceProvider"
      ],
      "aliases": {
        "Rakan": "TELstatic\\Rakan\\Facade"
      }
    }
  }
}
